#Items

class items:
    def __init__(self, totem, träsvärd, järnsvärd, potionnormal,potionepic):
        self.health = totem
        self.träsvärddamage = träsvärd
        self.järnsvärddamage = järnsvärd
        self.normalhealth = potionnormal
        self.epichealth = potionepic

player = items(100, 10, 20, 20, 50)

# Hur många items man har av varje
totem = "a"
a = 1

träsvärd = "b"
b = 0

järnsvärd = "c"
c = 1

potionnormal = "d"
d = 0

potionepic = "e"
e = 2

#Om spelaren trycker på en knapp då visar den hans/hennes inventory
# if player press (#)
if a > 0:
    print(f"Du har {a} totem")
else:
    print("Du har ingen totem")